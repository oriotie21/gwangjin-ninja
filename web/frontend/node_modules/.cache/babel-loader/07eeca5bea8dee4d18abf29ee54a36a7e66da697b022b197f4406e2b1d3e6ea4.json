{"ast":null,"code":"import L from \"leaflet\";\nimport \"leaflet/dist/leaflet.css\";\nexport default {\n  name: \"AttackForm\",\n  mounted() {\n    // const map = L.map(\"map\");\n    // map.setView([38, 157], 1.5);\n\n    const southWest = L.latLng(-90, 0); // 남서쪽 좌표\n    const northEast = L.latLng(90, 0); // 북동쪽 좌표\n    const bounds = L.latLngBounds(southWest, northEast); // 좌표 범위 객체 생성\n    const map = L.map(\"map\", {\n      maxBounds: bounds,\n      // 최대 좌표 범위 설정\n      maxBoundsViscosity: 1.0 // 지도 영역을 벗어날 때 바운드를 따르도록 설정\n    });\n\n    map.fitBounds(bounds); // 좌표 범위에 맞게 지도를 조정\n\n    L.tileLayer(\"https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png\", {\n      minZoom: 1.5,\n      maxZoom: 19,\n      attribution: 'Map data &copy; <a href=\"https://www.openstreetmap.org/\">OpenStreetMap</a> contributors'\n    }).addTo(map);\n  }\n};","map":{"version":3,"names":["L","name","mounted","southWest","latLng","northEast","bounds","latLngBounds","map","maxBounds","maxBoundsViscosity","fitBounds","tileLayer","minZoom","maxZoom","attribution","addTo"],"sources":["C:\\github\\GwangJin_Ninja\\frontend\\src\\components\\Attack.vue"],"sourcesContent":["<template>\r\n  <div id=\"map\"></div>\r\n</template>\r\n\r\n<script>\r\nimport L from \"leaflet\";\r\nimport \"leaflet/dist/leaflet.css\";\r\n\r\nexport default {\r\n  name: \"AttackForm\",\r\n  mounted() {\r\n    // const map = L.map(\"map\");\r\n    // map.setView([38, 157], 1.5);\r\n\r\n    const southWest = L.latLng(-90, 0); // 남서쪽 좌표\r\n    const northEast = L.latLng(90, 0); // 북동쪽 좌표\r\n    const bounds = L.latLngBounds(southWest, northEast); // 좌표 범위 객체 생성\r\n    const map = L.map(\"map\", {\r\n      maxBounds: bounds, // 최대 좌표 범위 설정\r\n      maxBoundsViscosity: 1.0, // 지도 영역을 벗어날 때 바운드를 따르도록 설정\r\n    });\r\n    map.fitBounds(bounds); // 좌표 범위에 맞게 지도를 조정\r\n\r\n    L.tileLayer(\"https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png\", {\r\n      minZoom: 1.5,\r\n      maxZoom: 19,\r\n      attribution:\r\n        'Map 