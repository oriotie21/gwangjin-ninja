{"ast":null,"code":"import { GChart } from \"vue-google-charts\";\nimport L from \"leaflet\";\nimport \"leaflet/dist/leaflet.css\";\nexport default {\n  name: \"AttackForm\",\n  components: {\n    GChart\n  },\n  mounted() {\n    const southWest = L.latLng(-40, -230); // 남서쪽 좌표\n    const northEast = L.latLng(70, -180); // 북동쪽 좌표\n    const bounds = L.latLngBounds(southWest, northEast); // 좌표 범위 객체 생성\n    const map = L.map(\"map\", {\n      maxBounds: bounds,\n      // 최대 좌표 범위 설정\n      maxBoundsViscosity: 1.0 // 지도 영역을 벗어날 때 바운드를 따르도록 설정\n    });\n\n    map.fitBounds(bounds); // 좌표 범위에 맞게 지도를 조정\n\n    L.tileLayer(\"https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png\", {\n      minZoom: 1.5,\n      maxZoom: 19,\n      attribution: 'Map data &copy; <a href=\"https://www.openstreetmap.org/\">OpenStreetMap</a> contributors'\n    }).addTo(map);\n  },\n  data() {\n    return {\n      data: [[\"Daily Routine\", \"Hours per Day\"], [\"Work\", 14], [\"Eat\", 1], [\"Reading\", 2], [\"Exercise\", 2], [\"Sleep\", 5]],\n      options: {\n        backgroundColor: \"#AFD3E2\",\n        width: 1000,\n        height: 600,\n        chartArea: {\n          width: \"90%\",\n          height: \"90%\"\n        },\n        colors: [\"#FFB6C1\", \"#ADD8E6\", \"#98FB98\", \"#FFDAB9\", \"#FFA07A\"],\n        pieSliceBorderColor: \"none\"\n      }\n    };\n  }\n};","map":{"version":3,"names":["GChart","L","name","components","mounted","southWest","latLng","northEast","bounds","latLngBounds","map","maxBounds","maxBoundsViscosity","fitBounds","tileLayer","minZoom","maxZoom","attribution","addTo","data","options","backgroundColor","width","height","chartArea","colors","pieSliceBorderColor"],"sources":["C:\\github\\GwangJin_Ninja\\frontend\\src\\components\\Attack.vue"],"sourcesContent":["<template>\r\n  <b-container fluid>\r\n    <b-row>\r\n      <div id=\"map\" class=\"col-md-8\"></div>\r\n      <div class=\"col-md-4\">\r\n        <GChart type=\"PieChart\" :options=\"options\" :data=\"data\" />\r\n      </div>\r\n    </b-row>\r\n  </b-container>\r\n</template>\r\n\r\n<script>\r\nimport { GChart } from \"vue-google-charts\";\r\nimport L from \"leaflet\";\r\nimport \"leaflet/dist/leaflet.css\";\r\n\r\nexport default {\r\n  name: \"AttackForm\",\r\n  components: {\r\n    GChart,\r\n  },\r\n  mounted() {\r\n    const southWest = L.latLng(-40, -230); // 남서쪽 좌표\r\n    const northEast = L.latLng(70, -180); // 북동쪽 좌표\r\n    const bounds = L.latLngBounds(southWest, northEast); // 좌표 범위 객체 생성\r\n    const map = L.map(\"map\", {\r\n      maxBounds: bounds, // 최대 좌표 범위 설정\r\n      maxBoundsViscosity: 1.0, // 지도 영역을 벗어날 때 바운드를 따르도록 설정\r\n    });\r\n    map.fitBounds(bounds); // 좌표 범위에 맞게 지도를 조정\r\n\r\n    L.tileLayer(\"https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png\", {\r\n      minZoom: 1.5,\r\n      maxZoom: 19,\r\n      attribution:\r\n        'Map data &copy; <a href=\"https://www.openstreetmap.org/\">OpenStreetMap</a> contributors',\r\n    }).addTo(map);\r\n  },\r\n  data() {\r\n    return {\r\n      data: [\r\n        [\"Daily Routine\", \"Hours per Day\"],\r\n        [\"Work\", 14],\r\n        [\"Eat\", 1],\r\n        [\"Reading\", 2],\r\n        [\"Exercise\", 2],\r\n        [\"Sleep\", 5],\r\n      ],\r\n      options: {\r\n        backgroundColor: \"#AFD3E2\",\r\n        width: 1000,\r\n        height: 600,\r\n        chartArea: {\r\n          width: \"90%\",\r\n          height: \"90%\",\r\n        },\r\n        colors: [\"#FFB6C1\", \"#ADD8E6\", \"#98FB98\", \"#FFDAB9\", \"#FFA07A\"],\r\n        pieSliceBorderColor: \"none\",\r\n      },\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n#map {\r\n  width: 1000px;\r\n  height: 600px;\r\n}\r\n</style>\r\n"],"mappings":"AAYA,SAASA,MAAK,QAAS,mBAAmB;AAC1C,OAAOC,CAAA,MAAO,SAAS;AACvB,OAAO,0BAA0B;AAEjC,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,OAAOA,CAAA,EAAG;IACR,MAAMC,SAAQ,GAAIJ,CAAC,CAACK,MAAM,CAAC,CAAC,EAAE,EAAE,CAAC,GAAG,CAAC,EAAE;IACvC,MAAMC,SAAQ,GAAIN,CAAC,CAACK,MAAM,CAAC