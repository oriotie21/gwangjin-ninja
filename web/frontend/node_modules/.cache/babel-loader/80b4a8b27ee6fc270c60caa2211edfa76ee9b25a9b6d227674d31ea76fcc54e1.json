{"ast":null,"code":"import axios from \"axios\";\nimport { reactive } from \"vue\";\nexport default {\n  setup() {\n    const state = reactive({\n      account: {\n        id: null,\n        username: \"\"\n      },\n      form: {\n        loginId: \"\",\n        loginPw: \"\"\n      }\n    });\n    const submit = () => {\n      const args = {\n        loginId: state.form.loginId,\n        loginPw: state.form.loginPw\n      };\n      axios.post(\"/api/account\", args).then(res => {\n        alert(\"로그인에 성공했습니다.\");\n        state.account = res.data;\n        state.form.loginId = \"\";\n        state.form.loginPw = \"\";\n      }).catch(() => {\n        alert(\"로그인에 실패했습니다.\");\n      });\n    };\n    const logout = () => {\n      axios.delete(\"/api/account\").then(() => {\n        alert(\"로그아웃하였습니다.\");\n        state.account.id = null;\n        state.account.username = \"\";\n      });\n    };\n    axios.get(\"/api/account\").then(res => {\n      state.account = res.data;\n    });\n    return {\n      state,\n      submit,\n      logout\n    };\n  }\n};","map":{"version":3,"names":["axios","reactive","setup","state","account","id","username","form","loginId","loginPw","submit","args","post","then","res","alert","data","catch","logout","delete","get"],"sources":["C:\\github\\GwangJin_Ninja\\frontend\\src\\App.vue"],"sourcesContent":["<template>\n  <div class=\"app\">\n    <div v-if=\"state.account.id\">\n      <p>안녕하세요 {{ state.account.username }}님!</p>\n      <button @click=\"logout()\">로그아웃</button>\n    </div>\n    <div v-else>\n      <label for=\"loginId\">\n        <!--label로 묶어줌-->\n        <span>아이디</span>\n        <input type=\"text\" id=\"loginId\" v-model=\"state.form.loginId\" />\n      </label>\n      <hr />\n      <label for=\"loginPw\">\n        <!--label로 묶어줌-->\n        <span>패스워드</span>\n        <input type=\"password\" id=\"loginPw\" v-model=\"state.form.loginPw\" />\n      </label>\n      <hr />\n      <button @click=\"submit()\">로그인</button>\n    </div>\n  </div>\n  <div>\n    <!-- /login 경로로 이동하는 링크를 만듭니다. -->\n    <router-link to=\"/login\">로그인</router-link>\n    <form v-if=\"!state.account.id\" @submit.prevent=\"submit\">\n      <!-- 로그인 폼의 내용 -->\n    </form>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport { reactive } from \"vue\";\nexport default {\n  setup() {\n    const state = reactive({\n      account: {\n        id: null,\n        username: \"\",\n      },\n      form: {\n        loginId: \"\",\n        loginPw: \"\",\n      },\n    });\n\n    const submit = () => {\n      const args = {\n        loginId: state.form.loginId,\n        loginPw: state.form.loginPw,\n      };\n      axios\n        .post(\"/api/account\", args)\n        .then((res) => {\n          alert(\"로그인에 성공했습니다.\");\n          state.account = res.data;\n          state.form.loginId = \"\";\n          state.form.loginPw = \"\";\n        })\n        .catch(() => {\n          alert(\"로그인에 실패했습니다.\");\n        });\n    };\n\n    const logout = () => {\n      axios.delete(\"/api/account\").then(() => {\n        alert(\"로그아웃하였습니다.\");\n        state.account.id = null;\n        state.account.username = \"\";\n      });\n    };\n\n    axios.get(\"/api/account\").then((res) => {\n      state.account = res.data;\n    });\n\n    return { state, submit, logout };\n  },\n};\n</script>\n>\n"],"mappings":"AAgCA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,QAAO,QAAS,KAAK;AAC9B,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIF,QAAQ,CAAC;MACrBG,OAAO,EAAE;QACPC,EAAE,EAAE,IAAI;QACRC,QAAQ,EAAE;MACZ,CAAC;MACDC,IAAI,EAAE;QACJC,OAAO,EAAE,EAAE;QACXC,OAAO,EAAE;MACX;IACF,CAAC,CAAC;IAEF,MAAMC,MAAK,GAAIA,CAAA,KAAM;MACnB,MAAMC,IAAG,GAAI;QACXH,OAAO,EAAEL,KAAK,CAACI,IAAI,CAACC,OAAO;QAC3BC,OAAO,EAAEN,KAAK,CAACI,IAAI,CAACE;MACtB,CAAC;MACDT,KAAI,CACDY,IAAI,CAAC,cAAc,EAAED,IAAI,EACzBE,IAAI,CAAEC,GAAG,IAAK;QACbC,KAAK,CAAC,cAAc,CAAC;QACrBZ,KAAK,CAACC,OAAM,GAAIU,GAAG,CAACE,IAAI;QACxBb,KAAK,CAACI,IAAI,CAACC,OAAM,GAAI,EAAE;QACvBL,KAAK,CAACI,IAAI,CAACE,OAAM