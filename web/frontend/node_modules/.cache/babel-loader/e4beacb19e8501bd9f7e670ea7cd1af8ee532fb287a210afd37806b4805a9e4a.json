{"ast":null,"code":"import Chart from \"chart.js\";\nexport default {\n  name: \"PieChart\",\n  mounted() {\n    this.renderPieChart();\n  },\n  methods: {\n    renderPieChart() {\n      const chartData = {\n        labels: [\"Red\", \"Green\", \"Blue\"],\n        datasets: [{\n          data: [300, 50, 100],\n          backgroundColor: [\"#FF6384\", \"#36A2EB\", \"#FFCE56\"],\n          hoverBackgroundColor: [\"#FF6384\", \"#36A2EB\", \"#FFCE56\"]\n        }]\n      };\n      const chartOptions = {\n        responsive: true,\n        maintainAspectRatio: false,\n        legend: {\n          display: true,\n          position: \"right\",\n          labels: {\n            fontSize: 14,\n            boxWidth: 15,\n            padding: 20\n          }\n        },\n        tooltips: {\n          callbacks: {\n            label: function (tooltipItem, data) {\n              const dataset = data.datasets[tooltipItem.datasetIndex];\n              const index = tooltipItem.index;\n              return dataset.labels[index] + \": \" + dataset.data[index];\n            }\n          }\n        }\n      };\n      const chartConfig = {\n        type: \"pie\",\n        data: chartData,\n        options: chartOptions\n      };\n      this.chart = new Chart(this.$refs.chart, chartConfig);\n    }\n  }\n};","map":{"version":3,"names":["Chart","name","mounted","renderPieChart","methods","chartData","labels","datasets","data","backgroundColor","hoverBackgroundColor","chartOptions","responsive","maintainAspectRatio","legend","display","position","fontSize","boxWidth","padding","tooltips","callbacks","label","tooltipItem","dataset","datasetIndex","index","chartConfig","type","options","chart","$refs"],"sources":["C:\\github\\GwangJin_Ninja\\frontend\\src\\components\\Attack.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <canvas ref=\"chart\"></canvas>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Chart from \"chart.js\";\r\n\r\nexport default {\r\n  name: \"PieChart\",\r\n 